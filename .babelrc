{
  "presets": [
    [
      "es2015",
      {
        "modules": false
      }
    ],
    "stage-0",
    "react"
  ],
  "env": {
    "development": {
      "plugins": [
        "react-hot-loader/babel",
        "syntax-dynamic-import",
        [
          "import",
          {
            "libraryName": "antd",
            "style":true
          }
        ]
      ]
    },
    "production": {
      "plugins": [
        [
          "import",
          {
            "libraryName": "antd",
            "style":true
          }
        ]
      ]
    }
  }
}
